preprocess <- function(ff) {
ff <- flowCore::compensate(ff, ff@description$"$SPILLOVER")
lgcl <- flowCore::logicleTransform( w = 0.5, m =4.5)
trans <- transformList(colnames(ff), lgcl)
ff <- flowCore::transform(ff,
trans)
#ff@exprs <- scale(ff@exprs)
ff
}
